@model WebApplication1.Data.GeoChange

<h2>Delete Geo Change</h2>

<p>Are you sure you want to delete this geo change?</p>

<div>
    <strong>Description:</strong> @Model.Description
</div>
<div>
    <strong>GeoJsonData:</strong> @Model.GeoJson
</div>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" name="id" value="@Model.Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="/Home/UpdateOverview" class="btn btn-secondary">Cancel</a>
</form>
